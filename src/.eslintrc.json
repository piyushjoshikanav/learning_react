{
  "extends": ["react-app", "react-app/jest"
    // , "prettier"
  ],
  "rules": {
    "eqeqeq": ["error"],
    "react/function-component-definition": [
      2,
      {
        "namedComponents": "function-declaration"
      }
    ],
    "@typescript-eslint/no-unused-vars": ["error"],
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "pathGroups": [
          {
            "pattern": "/**",
            "group": "external",
            "position": "after"
          }
        ]
      }
    ]
  }
}
